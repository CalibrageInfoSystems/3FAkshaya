<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white"
    tools:context=".views.fragments.contactfragment">

    <!-- TODO: Update blank fragment layout -->

    <ImageView
        android:layout_gravity="center"
        android:src="@drawable/ic_imp_contacts"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"/>
    <TextView
        style="@style/Common_textView_title"
        android:textColor="@color/colorOrange_head"
        android:gravity="center"
        android:id="@+id/head"
        android:text="@string/important_contacts"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"/>
    <RelativeLayout

        android:layout_gravity="top"
        android:id="@+id/lin_one"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerInParent="true"
        android:layout_marginStart="@dimen/_8dp"
        android:layout_marginLeft="@dimen/_8dp"
        android:layout_marginEnd="@dimen/_8dp"
        android:layout_marginRight="@dimen/_8dp"
        android:layout_marginBottom="@dimen/_8dp"
        >




        <LinearLayout
            android:id="@+id/lin_two"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@+id/lin_head"
            android:layout_marginTop="10sp"
            android:weightSum="1"
            android:orientation="horizontal">


            <TextView
                android:id="@+id/tvtext_item_two"
                android:layout_width="0dp"
                android:layout_height="wrap_content"

                android:layout_marginTop="15sp"
                android:fontFamily="@font/roboto_regular"
                android:text="@string/officer_name"
                android:textColor="@color/black"
                android:layout_weight="0.6"
                android:textSize="@dimen/album_title"
                android:textStyle="bold" />


            <TextView
                android:id="@+id/dot2"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_centerHorizontal="true"
                android:layout_marginTop="15sp"
                android:fontFamily="@font/roboto_regular"
                android:text=":"
                android:layout_weight="0.03"
                android:textColor="@color/black"
                android:textSize="@dimen/album_title"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/officer_name"
                android:layout_width="0dp"
                android:layout_height="wrap_content"

                android:layout_marginTop="15sp"
                android:layout_toRightOf="@+id/dot2"
                android:fontFamily="@font/roboto_regular"
                android:hint=""
                android:text=""
                android:layout_weight="0.37"
                android:textColor="@color/black"
                android:textSize="@dimen/album_title" />

        </LinearLayout>

        <LinearLayout
            android:id="@+id/lin_three"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@+id/lin_two"
            android:layout_marginTop="10sp"
            android:weightSum="1"
            android:orientation="horizontal">


            <TextView
                android:id="@+id/tvtext_item_three"
                android:layout_width="0dp"
                android:layout_height="wrap_content"

                android:layout_marginTop="5sp"
                android:fontFamily="@font/roboto_regular"
                android:text="@string/officer_mobile"
                android:textColor="@color/black"
                android:layout_weight="0.6"
                android:textSize="@dimen/album_title"
                android:textStyle="bold" />


            <TextView
                android:id="@+id/dot3"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_centerHorizontal="true"
                android:layout_marginTop="5sp"
                android:fontFamily="@font/roboto_regular"
                android:text=":"
                android:layout_weight="0.03"
                android:textColor="@color/black"
                android:textSize="@dimen/album_title"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/officer_mobile"
                android:layout_width="0dp"
                android:layout_height="wrap_content"

                android:layout_marginTop="5sp"
                android:layout_toRightOf="@+id/dot3"
                android:fontFamily="@font/roboto_regular"
                android:hint=""
                android:text=""
                android:layout_weight="0.37"
                android:textColor="@color/green"
                android:textSize="@dimen/album_title" />

        </LinearLayout>

        <LinearLayout
            android:id="@+id/lin_four"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@+id/lin_three"
            android:layout_marginTop="10sp"
            android:weightSum="1"
            android:orientation="horizontal">


            <TextView
                android:id="@+id/tvtext_item_six"
                android:layout_width="0dp"
                android:layout_height="wrap_content"

                android:layout_marginTop="5sp"
                android:layout_weight="0.6"
                android:fontFamily="@font/roboto_regular"
                android:text="@string/manager_name"
                android:textColor="@color/black"
                android:textSize="@dimen/album_title"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/dot4"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_centerHorizontal="true"
                android:layout_marginTop="5sp"
                android:layout_weight="0.03"
                android:text=":"
                android:textColor="@color/black"
                android:textSize="@dimen/album_title"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/manager_name"
                android:layout_width="0dp"
                android:layout_height="wrap_content"

                android:layout_marginTop="5sp"
                android:layout_weight="0.37"
                android:layout_toRightOf="@+id/dot4"
                android:fontFamily="@font/roboto_regular"
                android:hint=""
                android:text=""
                android:textColor="@color/black"
                android:textSize="@dimen/album_title" />

        </LinearLayout>
        <LinearLayout
            android:id="@+id/lin_fournew"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@+id/lin_four"
            android:layout_marginTop="10sp"
            android:weightSum="1"
            android:orientation="horizontal">


            <TextView
                android:id="@+id/text_manaager_contact"
                android:layout_width="0dp"
                android:layout_height="wrap_content"

                android:layout_weight="0.6"
                android:layout_marginTop="5sp"
                android:fontFamily="@font/roboto_regular"
                android:text="@string/manager_num"
                android:textColor="@color/black"
                android:textSize="@dimen/album_title"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/dotn"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_centerHorizontal="true"
                android:layout_marginTop="5sp"
                android:text=":"
                android:layout_weight="0.03"
                android:textColor="@color/black"
                android:textSize="@dimen/album_title"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/manager_mobile"
                android:layout_width="0dp"
                android:layout_height="wrap_content"

                android:layout_marginTop="5sp"
                android:layout_weight="0.37"
                android:layout_toRightOf="@+id/dotn"
                android:fontFamily="@font/roboto_regular"
                android:hint=""
                android:text=""
                android:textColor="@color/green"
                android:textSize="@dimen/album_title" />

        </LinearLayout>

        <LinearLayout
            android:id="@+id/lin_five"
            android:layout_width="match_parent"
            android:weightSum="1"
            android:layout_height="wrap_content"
            android:layout_below="@+id/lin_fournew"
            android:layout_marginTop="10sp"
            android:orientation="horizontal">


            <TextView
                android:id="@+id/tvtext_item_eight"
                android:layout_width="0dp"
                android:layout_height="wrap_content"

                android:layout_marginTop="5sp"
                android:text="@string/head_name"
                android:layout_weight="0.6"
                android:textColor="@color/black"
                android:textSize="@dimen/album_title"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/dot5"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_centerHorizontal="true"
                android:layout_marginTop="5sp"
                android:layout_weight="0.03"
                android:fontFamily="@font/roboto_regular"
                android:text=":"
                android:textColor="@color/black"
                android:textSize="@dimen/album_title"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/head_name"
                android:layout_width="0dp"
                android:layout_height="wrap_content"

                android:layout_marginTop="5sp"
                android:layout_weight="0.37"
                android:layout_toRightOf="@+id/dot5"
                android:fontFamily="@font/roboto_regular"
                android:hint=""
                android:text=""
                android:textColor="@color/black"
                android:textSize="@dimen/album_title" />

        </LinearLayout>

        <LinearLayout
            android:id="@+id/lin_six"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@+id/lin_five"
            android:layout_marginTop="10sp"
            android:weightSum="1"
            android:orientation="horizontal">


            <TextView
                android:id="@+id/tvtext_item_ten"
                android:layout_width="0dp"
                android:layout_height="wrap_content"

                android:layout_marginTop="5sp"
                android:layout_weight="0.6"
                android:fontFamily="@font/roboto_regular"
                android:text="@string/customer_care"
                android:textColor="@color/black"
                android:textSize="@dimen/album_title"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/dot6"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_centerHorizontal="true"
                android:layout_marginTop="5sp"
                android:text=":"
                android:layout_weight="0.03"
                android:textColor="@color/black"
                android:textSize="@dimen/album_title"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/care_number"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
             android:layout_marginTop="5sp"
                android:layout_toRightOf="@+id/dot6"
                android:fontFamily="@font/roboto_regular"
                android:hint=""
                android:layout_weight="0.37"
                android:textColor="@color/green"
                android:text=""

                android:textSize="@dimen/album_title" />


        </LinearLayout>

        <LinearLayout
            android:id="@+id/lin_seven"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@+id/lin_six"
            android:layout_marginTop="10sp"
            android:weightSum="1"
            android:orientation="horizontal">


            <TextView
                android:id="@+id/tvtext_item_te"
                android:layout_width="0dp"
                android:layout_height="wrap_content"

                android:layout_marginTop="5sp"
                android:layout_weight="0.6"
                android:fontFamily="@font/roboto_regular"
                android:text="@string/whats_number"
                android:textColor="@color/black"
                android:textSize="@dimen/album_title"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/dot7"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_centerHorizontal="true"
                android:layout_marginTop="5sp"
                android:text=":"
                android:layout_weight="0.03"
                android:textColor="@color/black"
                android:textSize="@dimen/album_title"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/whatsapp"
                android:layout_width="0dp"
                android:layout_height="wrap_content"

                android:layout_marginTop="5sp"
                android:layout_toRightOf="@+id/dot7"
                android:fontFamily="@font/roboto_regular"
                android:hint=""
                android:layout_weight="0.37"
                android:textColor="@color/green"
                android:text=""

                android:textSize="@dimen/album_title" />


        </LinearLayout>


    </RelativeLayout>

</LinearLayout>